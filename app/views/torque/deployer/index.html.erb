<%= render partial: 'page_header' %>
<h2>Pull Requests</h2>
<div class="row">
  <div class="col-sm-6">
    <%= render partial: 'pull_requests' %>
  </div>
  <div class="col-sm-4">
    <div class="panel panel-success">
      <div class="panel-heading">
        <h3 class="panel-title">Branches</h3>
      </div>
      <div class="panel-body">
        <%= render partial: 'branches' %>
      </div>
    </div>
  </div>
  <div class="col-sm-2">
    <div class="panel panel-danger">
      <div class="panel-heading">
        <h3 class="panel-title">Tags</h3>
      </div>
      <div class="panel-body">
        <%= render partial: 'tags' %>
      </div>
    </div>
  </div>
</div>
